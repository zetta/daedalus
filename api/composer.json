{
    "name": "zetta/daedalus",
    "description": "Shortest driving distance finder",
    "type": "project",
    "authors": [
        {
            "name": "zetta",
            "email": "zetaweb@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "silex/silex": "~2.0",
        "doctrine/dbal": "~2.2",
        "php-amqplib/php-amqplib": "2.7.*",
        "ramsey/uuid": "^3.7",
        "egeloen/google-map": "^2.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "egeloen/serializer": "^1.0",
        "symfony/console": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Daedalus\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ],
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
